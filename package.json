    "minify",
    "static"
  ],
  "author": "diegopluna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegopluna/vite-plugin-minify-static/issues"
  },
  "homepage": "https://github.com/diegopluna/vite-plugin-minify-static#readme",
  "devDependencies": {
    "@types/node": "^24.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
